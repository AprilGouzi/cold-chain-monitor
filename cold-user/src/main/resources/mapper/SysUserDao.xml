<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.chain.cold.common.user.dao.SysUserDao">

    <select id="queryByUserName" resultType="com.chain.cold.common.user.entity.SysUserEntity">
        SELECT *
        FROM sys_user
        WHERE username = #{username}
    </select>

</mapper>